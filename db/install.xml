<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/coursesize/db" VERSION="2012070200" COMMENT="XMLDB file for Moodle report/coursesize"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_coursesize" COMMENT="Coursesize calculations cache.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="contextlevel"/>
        <FIELD NAME="contextlevel" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Context level (course, category etc.)" PREVIOUS="id" NEXT="instanceid"/>
        <FIELD NAME="instanceid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Course/category etc. id" PREVIOUS="contextlevel" NEXT="filesize"/>
        <FIELD NAME="filesize" TYPE="int" LENGTH="15" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Context size in bytes" PREVIOUS="instanceid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
	  <INDEXES>
        <INDEX NAME="contextlevel" FIELDS="contextlevel" UNIQUE="false" NEXT="instanceid"/>
        <INDEX NAME="instanceid" FIELDS="instanceid" UNIQUE="false" PREVIOUS="contextlevel"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
